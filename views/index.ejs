<% include partials/header %>


<% users.forEach((user) => {  %>
    <div id="user-container">
        <h1><%= user.name %></h1>
        <h1><%= user.age %></h1>
    </div>

    <div id="posts-container">
        <% user.posts.forEach((post) => {  %>
            <p><%= post.title %></p>
            <p><%= post.content %></p>
        <% })  %>
    </div>
<% })  %>


<% include partials/footer %>
